#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int * f(int i)
{
    int s[5] = {1, 2, 3, 4, 5};
    return &s[i];
}

int main(void)
{
    int * a;
    a = f(3);           // dangling pointer (f returns the address of the variable in f, which is only valid in f)
    printf("%p\n", a);
    return 0;
}
